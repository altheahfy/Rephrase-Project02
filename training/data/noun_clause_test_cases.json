{
  "meta": {
    "title": "名詞節テストケース - NounClauseHandler機能検証用",
    "total_count": 8,
    "grammar_category": "noun_clauses",
    "patterns": [
      "that節（目的語）",
      "that節（主語）", 
      "wh-節（目的語）",
      "wh-節（主語）",
      "間接疑問文",
      "whether節",
      "if節（名詞用法）",
      "複合名詞節"
    ],
    "note": "REPHRASE_SLOT_STRUCTURE_MANDATORY_REFERENCE.md準拠。実装表記はsub-*だが概念的にはO1-sub-*等"
  },
  "test_cases": {
    "113": {
      "V_group_key": "think",
      "grammar_category": "noun_clauses",
      "sentence": "I think that he is smart.",
      "expected": {
        "main_slots": {
          "S": "I",
          "V": "think",
          "O1": ""
        },
        "sub_slots": {
          "sub-s": "that he",
          "sub-v": "is",
          "sub-c1": "smart"
        }
      }
    },
    "114": {
      "V_group_key": "surprise",
      "grammar_category": "noun_clauses", 
      "sentence": "That he succeeded surprised everyone.",
      "expected": {
        "main_slots": {
          "S": "",
          "V": "surprised",
          "O1": "everyone"
        },
        "sub_slots": {
          "sub-s": "that he",
          "sub-v": "succeeded"
        }
      }
    },
    "115": {
      "V_group_key": "know",
      "grammar_category": "noun_clauses",
      "sentence": "I know what you mean.",
      "expected": {
        "main_slots": {
          "S": "I",
          "V": "know",
          "O1": ""
        },
        "sub_slots": {
          "sub-s": "you",
          "sub-v": "mean",
          "sub-o1": "what"
        }
      }
    },
    "116": {
      "V_group_key": "be",
      "grammar_category": "noun_clauses",
      "sentence": "Where he lives is unknown.",
      "expected": {
        "main_slots": {
          "S": "",
          "V": "is",
          "C1": "unknown"
        },
        "sub_slots": {
          "sub-s": "he",
          "sub-v": "lives",
          "sub-m2": "where"
        }
      }
    },
    "117": {
      "V_group_key": "ask",
      "grammar_category": "noun_clauses",
      "sentence": "She asked me why I was late.",
      "expected": {
        "main_slots": {
          "S": "She",
          "V": "asked",
          "O1": "me",
          "O2": ""
        },
        "sub_slots": {
          "sub-s": "I",
          "sub-v": "was",
          "sub-c1": "late",
          "sub-m2": "why"
        }
      }
    },
    "118": {
      "V_group_key": "wonder",
      "grammar_category": "noun_clauses",
      "sentence": "I wonder whether he will come.",
      "expected": {
        "main_slots": {
          "S": "I", 
          "V": "wonder",
          "O1": ""
        },
        "sub_slots": {
          "sub-s": "whether he",
          "sub-v": "will come"
        }
      }
    },
    "119": {
      "V_group_key": "depend",
      "grammar_category": "noun_clauses",
      "sentence": "It depends on if you agree.",
      "expected": {
        "main_slots": {
          "S": "It",
          "V": "depends",
          "M2": ""
        },
        "sub_slots": {
          "sub-s": "on if you",
          "sub-v": "agree"
        }
      }
    },
    "120": {
      "V_group_key": "believe",
      "grammar_category": "noun_clauses",
      "sentence": "I believe what he told me is true.",
      "expected": {
        "main_slots": {
          "S": "I",
          "V": "believe",
          "O1": ""
        },
        "sub_slots": {
          "sub-s": "what he told me",
          "sub-v": "is",
          "sub-c1": "true"
        }
      }
    }
  }
}
