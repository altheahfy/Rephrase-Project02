          "V": "surprised",
          "O1": "everyone"
        },
        "sub_slots": {
          "noun_clause": {
            "type": "that_clause",
            "role": "subject",
            "content": "he succeeded",
            "connector": "that",
            "internal_structure": {
              "S": "he",
              "V": "succeeded"
            }
          }
        }
      }
    },
