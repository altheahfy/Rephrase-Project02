  "test_cases": {
    "113": {
      "V_group_key": "think",
      "grammar_category": "noun_clauses",
      "pattern": "that節（目的語）",
      "sentence": "I think that he is smart.",
      "expected": {
        "main_slots": {
          "S": "I",
          "V": "think",
          "O1": "that he is smart"
        },
        "sub_slots": {
          "noun_clause": {
            "type": "that_clause",
            "role": "object",
            "content": "he is smart",
            "connector": "that",
            "internal_structure": {
              "S": "he",
              "V": "is", 
              "C1": "smart"
            }
          }
        }
      }
    },
    "114": {
