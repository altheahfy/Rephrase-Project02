{
  "name": "rephrase-project",
  "version": "1.0.0",
  "description": "Rephrase English Learning System",
  "main": "index.html",
  "scripts": {
    "dev": "npx http-server -p 3000 -c-1 --cors",
    "https": "npx http-server -p 3000 -c-1 --cors -S -C cert.pem -K key.pem -a 0.0.0.0",
    "create-cert": "npx mkcert -install && npx mkcert localhost 127.0.0.1 ::1 192.168.0.154",
    "serve": "npm run create-cert && npm run https"
  },
  "keywords": ["english", "learning", "voice", "training"],
  "author": "Rephrase Team",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "mkcert": "^3.2.0"
  }
}
